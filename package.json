{
	"name": "potato-bot",
	"version": "2.0.0",
	"description": "POTATO bot features loads of jokes, Minecraft integrations, emoji stealing and basic moderation.",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"build-w": "tsc --watch",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"start-b": "pnpm run build&&pnpm run start",
		"start-d": "cross-env NODE_ENV=development node dist/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edazpotato/POTATO.git"
	},
	"keywords": [
		"typescript",
		"discord",
		"discord-bot",
		"discord-js",
		"discord-akairo"
	],
	"author": "Edazpotato",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/edazpotato/POTATO/issues"
	},
	"homepage": "https://github.com/edazpotato/POTATO#readme",
	"dependencies": {
		"bufferutil": "^4.0.3",
		"cross-env": "^7.0.3",
		"discord-akairo": "^8.1.0",
		"discord.js": "^12.5.3",
		"dotenv-extended": "^2.9.0",
		"erlpack": "github:discord/erlpack",
		"sqlite": "^4.0.23",
		"sqlite3": "^5.0.2",
		"typescript": "^4.3.4",
		"utf-8-validate": "^5.0.5",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@types/node": "^15.12.4"
	}
